<div class="container-fluid">
  <div class="row">
    <div class="col-md-4"></div>
    <div class="col-md-4">
      <% @users.each do |u| %>
        <li>
            <strong> <%= u.id %> </strong><br />
            <%= u.username %><br />
            <%= u.email %><br />
             <%= link_to "More details!", user_path(u) %>
        </li>
      <% end %>
    </div>
    <div class="col-md-4"></div>
  </div>
</div>